{
  "name": "j2c",
  "version": "1.0.0-4",
  "description": "A tiny CSS in JS solution.",
  "main": "dist/j2c.commonjs.js",
  "module": "src/main.js",
  "scripts": {
    "preversion": "yarn run all",
    "prepublish": "(in-publish && git push --follow-tags) || not-in-publish",
    "install-plugins": "node scripts/install-plugins",
    "all": "yarn run build && yarn run lint && yarn run cover",
    "dev": "yarn run build && yarn run test",
    "build": "rm -f dist/*.js && node scripts/build.js",
    "lint": "eslint --fix .",
    "test": "ospec",
    "cover": "istanbul cover --print both node_modules/.bin/ospec && yarn run check-coverage",
    "check-coverage": "istanbul check-coverage --branches -9",
    "travis": "yarn install-plugins && yarn run build && istanbul cover --print both node_modules/.bin/ospec && (codecov || true) && yarn run  check-coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/j2css/j2c.git"
  },
  "keywords": [
    "CSS",
    "css-in-js"
  ],
  "author": "Pierre-Yves GÃ©rardy",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/j2css/j2c/issues"
  },
  "homepage": "http://j2c.py.gy",
  "devDependencies": {
    "cash-true": "0.0.2",
    "codecov": "^1.0.1",
    "commonmark": "^0.27.0",
    "compose-regexp": "^0.1.8",
    "eslint": "^3.12.0",
    "in-publish": "^2.0.0",
    "istanbul": "^0.4.2",
    "jsdom": "^9.8.3",
    "ospec": "^1.2.1",
    "perfectionist": "^2.1.2",
    "postcss": "^5.0.14",
    "postcss-minify-params": "^1.0.4",
    "postcss-minify-selectors": "^2.0.2",
    "rollup": "^0.36.4",
    "uglify-js": "^2.6.0",
    "yarn": "^0.18.1"
  }
}
